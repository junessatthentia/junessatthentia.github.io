block tableButtons
	include ../../components/table/_table-header-buttons-5.pug

block pageContent
	.table-responsive
		table.table.table-hover
			thead
				tr
					+thSortable('Summary')
					+thSortable('Registration')
					+thSortable('Requested Class')
					+thSortable('Requested Status')
					+thSortable('Submitted Date')
					+thSortable('Status')
