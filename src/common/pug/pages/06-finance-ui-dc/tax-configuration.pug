					+thSortable('Supplemental Tax Rate (%)')
					+thSortable('Default')
					+thSortable('State/Province/Territory')
					+thSortable('Modified On')
					+thSortable('Modified By')
			tbody
				tr
					td Ontario Tax
					td GST
					td 10.00
					td HST
					td !.00
